{
    "name": "org_heigl/date-formatter-i18n",
    "description": "Extendable Library to format DateTimes using Zend-Frameworks Date-Constants",
    "keywords": ["date","datetime","format"],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Heigl",
            "email": "andreas@heigl.org"
        }
    ],
    "require": {
        "org_heigl/date-formatter": "^1.1",
        "ext-intl" : "*",
        "php" : "^5.6|^7.0"
    },
    "autoload": {
        "psr-4" : {
            "Org_Heigl\\DateFormatterI18n\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Org_Heigl\\DateFormatterI18nTest\\" : "tests"
        }
    }
}
